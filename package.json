{
  "name": "@novout/vue-pdfmake",
  "author": "Novout",
  "license": "MIT",
  "version": "1.0.0",
  "description": "A pdfmake plugin for Vue 3",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Novout/vue-pdfmake"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src/**/*.{js,ts,tsx} --fix && pretty-quick",
    "docs": "typedoc"
  },
  "keywords": [
    "typescript",
    "rollup"
  ],
  "dependencies": {
    "makepdf": "^0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@types/jsdom": "^16.2.6",
    "@types/node": "^14.14.22",
    "@types/pdfmake": "^0.1.19",
    "@types/rollup": "^0.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rollup": "^2.38.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "typedoc": "^0.20.29",
    "typescript": "^4.1.3",
    "vue": "^3.2.4"
  },
  "files": [
    "lib",
    "package.json"
  ],
  "engines": {
    "node": ">= 12"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint",
      "git add"
    ]
  }
}
