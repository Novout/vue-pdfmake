{
  "name": "vue3-pdfmake",
  "author": "Novout",
  "license": "MIT",
  "version": "0.1.2",
  "description": "A pdfmake plugin for Vue 3",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/betterwrite/vue-pdfmake"
  },
  "scripts": {
    "build": "unbuild",
    "patch": "generi log patch",
    "minor": "generi log minor",
