      "optional": true
    }
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.6",
    "@types/node": "^14.14.22",
    "@types/pdfkit": "^0.12.6",
    "@types/pdfmake": "^0.1.21",
    "@vitejs/plugin-vue": "^2.3.2",
    "generi": "0.3.0",
    "typescript": "^4.1.3",
    "unbuild": "0.7.4",
    "vite": "2.9.8",
    "vue": "^3.0.0"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">= 12"
